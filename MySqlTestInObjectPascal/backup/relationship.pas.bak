unit Relationship;

{$mode objfpc}{$H+}

interface

uses
  Classes, SysUtils;

var
  // The id field of the Relationship class.
  idField: Integer;
  // The name field of the Relationship class.
  nameField: String;
  // The isDeleted field of the Relationship class.
  isDeletedField: Boolean;

implementation

{ Initializes a new instance of the Relationship class. }
procedure InitRelationship();
begin

end;

{ Initializes a new instance of the Relationship class. }
procedure InitRelationship(id: Integer; name: String; isdeleted: Boolean);
begin
     idField := id;
     nameField := name;
     isDeletedField := isdeleted;
end;

{ Gets the id. }
function getId(): Integer;
begin
     getId := idField;
end;

{ Sets the id. }
procedure setId(id: Integer);
begin
     idField := id;
end;

{ Gets the name. }
function getName(): String;
begin
     getName := nameField;
end;

{ Sets the name. }
procedure setName(name: String);
begin
     nameField := name;
end;

{ Gets the isDeleted. }
function getIsDeleted(): Boolean;
begin
     getIsDeleted := isDeletedField;
end;

{ Sets the isDeleted. }
procedure setIsDeleted(isdeleted: Boolean);
begin
     isDeletedField := isdeleted;
end;

end.

